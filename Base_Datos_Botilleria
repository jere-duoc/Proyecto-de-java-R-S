create database botilleria;
use botilleria;

create table Producto(
idProducto int not null auto_increment,
nombreProducto varchar(30) not null,
precioProducto int not null,
stock int not null,
tipoProducto varchar(30) not null,
primary key(idProducto)

);

create table Boleta(
idBoleta int not null auto_increment,
neto int not null,
totalBoleta int not null,
fecha date not null,

primary key(idBoleta)
)

-- drop table Producto;
-- drop table Boleta;
